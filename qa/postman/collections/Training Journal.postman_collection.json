						"exec": [
							"const createRep = (weight, repNumber) => {",
							"    return {",
							"        \"weight\": weight,",
							"        \"repNumber\": repNumber",
							"    };",
							"};",
							"",
							"const createSet = (setNumber) => {",
							"    return {",
							"        \"setNumber\": setNumber,",
							"        \"reps\": []",
							"    };",
							"};",
							"",
							"const createExercise = (name, position) => {",
							"    return {",
							"        \"name\": name,",
							"        \"position\": position,",
							"        \"sets\": []",
							"    };",
							"};",
							"",
							"const createWorkout = (name) => {",
							"    return {",
							"        \"name\": name,",
							"        \"date\": new Date().toISOString(),",
							"        \"exercises\": []",
							"    };",
							"};",
							"",
							"// --- 2. SMART HELPER (Accepts Array of Weights) ---",
							"// usage: addSets(exercise, repsPerSet, [weight1, weight2, weight3])",
							"const addSets = (exercise, reps, weights) => {",
							"    // We loop through the list of weights you provided",
							"    for (let i = 0; i < weights.length; i++) {",
							"        let currentWeight = weights[i];",
							"        let setNum = i + 1; // Sets start at 1",
							"        let set = createSet(setNum);",
							"        // Add the reps with the specific weight for THIS set",
							"        set.reps.push(createRep(currentWeight, reps));",
							"        exercise.sets.push(set);",
							"    }",
							"};",
							"",
							"createWorkoutRequest = function (pm) {",
							"    // Create workou",
							"    let myWorkout = createWorkout(\"Leg Day\");",
							"",
							"    // Create exercises",
							"    let legExtensions = createExercise(\"Leg exstensions\", 1);",
							"    let inclineLegPress = createExercise(\"Incline leg Press\", 2);",
							"    let walkingLunges = createExercise(\"Dumbbell Walking Lunges:\", 3);",
							"    let romanianDeadlift = createExercise(\"Dumbbell Romanian Deadlift\", 4);",
							"    let calfRaises = createExercise(\"Standing Machine Calf Raises\", 5);",
							"    let hipAbductions = createExercise(\"Machine Hip Abductions\", 6);",
							"",
							"    // Leg Extensions: 4 sets of 20 reps. Weights:",
							"    addSets(legExtensions, 20, [43, 43, 50, 50]);",
							"",
							"    // Leg Press: 4 sets of 15 reps.",
							"    addSets(inclineLegPress, 15, [40, 40, 40, 50]);",
							"",
							"    // Lunges: 3 sets of 20 steps. Constant weight",
							"    addSets(walkingLunges, 20, [12.5, 12.5, 12.5]);",
							"",
							"    // RDL: 3 sets of 15 reps. Increasing weight.",
							"    addSets(romanianDeadlift, 15, [30, 30, 30]);",
							"",
							"    // Calf Raises: 4 sets of 20 reps.",
							"    addSets(calfRaises, 20, [10, 10, 10, 10]);",
							"",
							"    // Hip Abductions: 3 sets of 20 reps.",
							"    addSets(hipAbductions, 20, [63, 63, 71]);",
							"",
							"    // Add to workout",
							"    myWorkout.exercises.push(legExtensions, inclineLegPress, walkingLunges, romanianDeadlift, calfRaises, hipAbductions);",
							"",
							"",
							"    pm.collectionVariables.set(\"bodyRequest\", JSON.stringify(myWorkout));",
							"",
							"}"
						]
					}
